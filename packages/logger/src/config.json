{
  "logging": {
    "logger": null,
    "logLevel": "info",
    "logFile": null,
    "format": {
      "type": "simple"
    },
    "transports": [{
      "type":"DailyRotateFile",
      "options": {
        "zippedArchive": true,
        "dirname":"./logs",
        "filename": "twake-%DATE%.log",
        "maxSize": "10m",
        "maxFiles": "5d"
      }
    }],
    "levels": null,
    "silent": false,
    "exitOnError": true,
    "defaultMeta": null,
    "exceptionHandlers": [],
    "rejectionHandlers": []
  }
}